<div class="container">
    <h3>View Employee:</h3>
    <form>
        <div class="form-group">
            <label for="exampleInputEmail1">First name</label>
            <input type="text" class="form-control" id="exampleInputEmail1" placeholder="First Name" ng-model="employee[0].name">
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Last name</label>
            <input type="text"  class="form-control" id="exampleInputPassword1" placeholder="Last name" ng-model="employee[0].last_name">
        </div>
        <button type="submit" ng-click="cancel()" class="btn btn-primary">Cancel</button>
    </form>
</div>